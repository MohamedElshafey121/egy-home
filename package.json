{
  "name": "egy-home",
  "proxy": "http://127.0.0.1:5000",
  "version": "1.0.0",
  "description": "\"APIs for graduation project\"",
  "main": "server.js",
  "engines": {
    "node": "12.16.1",
    "npm": "6.13.4"
  },
  "scripts": {
    "start": "node backEnd/server.js",
    "start:prod": "set NODE_ENV=production && nodemon backEnd/server.js",
    "server": "nodemon backEnd/server",
    "client": "npm start --prefix frontend",
    "client:dash": "npm start --prefix dashboard-production",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "dev:dash": "concurrently \"npm run server\" \"npm run client:dash\" ",
    "debug": "ndb server.js"
  },
  "author": "\"Mohamed Elshafey, Shrouk Ahmed, Samaa Ehab\"",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chroma-js": "^2.1.2",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.0",
    "passport": "^0.5.2",
    "passport-google-oauth2": "^0.2.0",
    "passport-google-oauth20": "^2.0.0",
    "reactjs-tag-input": "^2.0.15",
    "request": "^2.88.2",
    "sharp": "^0.28.3",
    "validator": "^13.6.0",
    "validators": "^0.3.1"
  },
  "devDependencies": {
    "concurrently": "^6.2.0"
  }
